<?xml version="1.0" encoding="utf-8"?>
<jnlp spec="7.0" codebase="$$codebase" href="launch.jnlp">
    <information>
        <title>jws-backend</title>
        <vendor>lrozek</vendor>
    </information>
    <update check="always" policy="always" />
    <security>
        <all-permissions />
    </security>
    <resources>
        $dependencies
    </resources>

    <!-- SWT Windows 32-bit native libraries -->
    <resources os="Windows" arch="x86">
        <jar href="org.eclipse.swt.win32.win32.x86.jar" version="$$swtVersionPlaceHolder" />
    </resources>
    <resources os="Windows" arch="i386">
        <jar href="org.eclipse.swt.win32.win32.x86.jar" version="$$swtVersionPlaceHolder" />
    </resources>

    <!-- SWT Windows 64-bit native libraries -->
    <resources os="Windows" arch="amd64">
        <jar href="org.eclipse.swt.win32.win32.x86_64.jar" version="$$swtVersionPlaceHolder" />
    </resources>
    <resources os="Windows" arch="x86_64">
        <jar href="org.eclipse.swt.win32.win32.x86_64.jar" version="$$swtVersionPlaceHolder" />
    </resources>

    <!-- SWT Linux 64-bit native libraries -->
    <resources os="Linux" arch="amd64">
        <jar href="org.eclipse.swt.gtk.linux.x86_64.jar" version="$$swtVersionPlaceHolder" />
    </resources>
    <resources os="Linux" arch="x86_64">
        <jar href="org.eclipse.swt.gtk.linux.x86_64.jar" version="$$swtVersionPlaceHolder" />
    </resources>


    <application-desc main-class="$mainClass" />
</jnlp>